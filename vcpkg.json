{
  "name": "sandbox",
  "version": "0.1.0",
  "dependencies": [
    {
      "name": "daxa",
      "features": [
        "utils-pipeline-manager-glslang",
        "utils-pipeline-manager-slang",
        "utils-task-graph",
        "utils-imgui"
      ]
    },
    "glfw3",
    {
      "name": "imgui",
      "features": [
        "glfw-binding",
        "docking-experimental"
