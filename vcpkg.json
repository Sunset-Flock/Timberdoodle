    },
    "glm",
    "fastgltf",
    "fmt",
    "ktx",
    "freeimage",
    "nlohmann-json",
    "implot"
  ],
  "builtin-baseline": "cf035d9916a0a23042b41fcae7ee0386d245af08",

  "vcpkg-configuration": {
    "overlay-ports": [
      "deps/daxa"
    ]
  }
}
